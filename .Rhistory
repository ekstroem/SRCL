plot(model$train_performance, type='l',yaxs='i', ylab="Mean squared error",
xlab="Epochs",main="Performance")
# Model visualisation
par(mar=c(0,0,0,0))
SRCL_3_plot_neural_network(model,names(exposure_data),5/max(model[[1]]))
# AUC
